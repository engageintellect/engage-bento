---
import BasicLayout from "./BasicLayout.astro";
import { ViewTransitions } from "astro:transitions";

interface Props {
  title: string;
  description: string;
}

const { title, description } = Astro.props;
---

<ViewTransitions />

<BasicLayout title={title} description={description}>
  <div slot="" class=""></div>
  <slot />
</BasicLayout>
